<h1>Users</h1>

<table>
  <tr>
    <th>login</th>
    <th>Name</th>
    <th>Type</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
        <td><%= link_to user.login, user_path(user) %></td>
        <td><%= user.name %></td>
        <td><%= user.type %></td>
    </tr>
  <% end %>
</table>
